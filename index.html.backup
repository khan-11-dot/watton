                if (username === 'admin' && password === 'admin123') {
                    // Hide login container
                    document.querySelector('.login-container').style.display = 'none';
                    // Show dashboard
                    adminDashboard.style.display = 'block';
                    // Update header
                    document.querySelector('.page-header .lead').textContent = 'Welcome to the Admin Dashboard';
                } else {
                    // Show error message
                    const alert = document.querySelector('.alert');
                    alert.classList.remove('alert-info');
                    alert.classList.add('alert-danger');
                    alert.textContent = 'Invalid username or password. Please try again.';
                }
            });

            // Close lists when clicking outside
            document.addEventListener('click', function(event) {
                if (!event.target.closest('.location-list') && !event.target.closest('.nav-link')) {
                    document.querySelectorAll('.location-list').forEach(list => {
                        list.style.display = 'none';
                    });
                }
            });
        });

        // Current registration being processed
        let currentRow = null;

        // Accept labor registration
        function acceptLabor(button) {
            currentRow = button.closest('tr');
            const name = currentRow.cells[0].textContent;
            document.querySelector('#acceptModal .modal-title').textContent = `Complete Registration - ${name}`;
        }

        // Reject labor registration
        function rejectLabor(button) {
            const row = button.closest('tr');
            if (confirm('Are you sure you want to reject this registration? This action cannot be undone.')) {
                row.remove();
                updatePendingCount();
                showNotification('Registration rejected and deleted successfully', 'danger');
            }
        }

        // Complete registration after collecting additional info
        function finalizeRegistration() {
            const form = document.getElementById('completeRegistrationForm');
            if (form.checkValidity()) {
                const phone = form.querySelector('input[type="tel"]').value;
                const location = form.querySelector('textarea').value;
                const warehouse = form.querySelector('select').value;

                if (!phone || !location) {
                    alert('Please fill in all required fields');
                    return;
                }

                // Update status in the table
                currentRow.cells[4].innerHTML = '<span class="badge bg-success">Accepted</span>';
                
                // Remove action buttons
                currentRow.cells[5].innerHTML = `
                    <span class="text-success"><i class="fas fa-check-circle"></i> Completed</span>
                `;

                // Close modal
                const modal = bootstrap.Modal.getInstance(document.getElementById('acceptModal'));
                modal.hide();

                // Reset form
                form.reset();

                // Update pending count
                updatePendingCount();

                // Show success notification
                showNotification('Registration completed successfully', 'success');
            } else {
                form.reportValidity();
            }
        }

        // Update pending count
        function updatePendingCount() {
            const pendingCount = document.querySelectorAll('#pendingRegistrationsTable tr .badge.bg-warning').length;
            document.getElementById('pendingCount').textContent = pendingCount;
            document.getElementById('quickActionPendingCount').textContent = pendingCount;
        }

        // Show notification
        function showNotification(message, type) {
            const notification = document.createElement('div');
            notification.className = `alert alert-${type} notification`;
            notification.style.cssText = `
                position: fixed;
                top: 20px;
                right: 20px;
                z-index: 1050;
                animation: slideIn 0.5s ease-out;
            `;
            notification.innerHTML = message;
            document.body.appendChild(notification);

            setTimeout(() => {
                notification.style.animation = 'slideOut 0.5s ease-in';
                setTimeout(() => notification.remove(), 500);
            }, 3000);
        }

        // Add notification animations to style
        const style = document.createElement('style');
        style.textContent = `
            @keyframes slideIn {
                from { transform: translateX(100%); }
                to { transform: translateX(0); }
            }
            @keyframes slideOut {
                from { transform: translateX(0); }
                to { transform: translateX(100%); }
            }
        `;
        document.head.appendChild(style);

        function showPendingRegistrations() {
            const modal = new bootstrap.Modal(document.getElementById('pendingRegistrationsModal'));
            modal.show();
        }

        function generateContactForm(button) {
            const row = button.closest('tr');
            const laborName = row.cells[0].textContent;
            
            if (confirm(`Are you sure you want to accept ${laborName}'s registration?`)) {
                // Update status to Accepted
                row.cells[4].innerHTML = '<span class="badge bg-success">Accepted</span>';
                
                // Update actions column to show View Details button only
                row.cells[5].innerHTML = `
                    <button class="btn btn-info btn-sm" onclick="viewLaborDetails(this)">
                        <i class="fas fa-eye"></i> View Details
                    </button>
                `;
                
                // Show success notification
                showNotification(`${laborName}'s registration has been accepted`, 'success');
                
                // Update pending count
                updatePendingCount();
                
                // Add to Recent Activity
                addToRecentActivity(`${laborName}'s registration was accepted`);
            }
        }

        // Add activity to Recent Activity section
        function addToRecentActivity(message) {
            const activityList = document.querySelector('.activity-list');
            const newActivity = document.createElement('div');
            newActivity.className = 'activity-item pb-3 mb-3 border-bottom';
            newActivity.innerHTML = `
                <p class="mb-1"><strong>Labor Registration</strong></p>
                <p class="text-muted mb-0">${message}</p>
                <small class="text-muted">Just now</small>
            `;
            
            // Add new activity at the top
            activityList.insertBefore(newActivity, activityList.firstChild);
            
            // Remove oldest activity if more than 3
            if (activityList.children.length > 3) {
                activityList.removeChild(activityList.lastChild);
            }
        }

        // View labor details
        function viewLaborDetails(button) {
            currentRow = button.closest('tr');
            const laborData = {
                name: currentRow.cells[0].textContent,
                cnic: currentRow.cells[1].textContent,
                age: currentRow.cells[2].textContent,
                experience: currentRow.cells[3].textContent,
                // Demo data - in real app, this would come from database
                gender: 'Male',
                marital: 'Married',
                skills: 'Hand Stemming, Tobacco Processing',
                prevWork: 'ABC Tobacco Company (2 years)',
                education: 'Matriculation',
                languages: 'Urdu, Pashto',
                health: 'Good',
                emergencyName: 'Usman Ali',
                emergencyRelation: 'Brother'
            };

            // Fill in the details
            document.getElementById('laborDetailName').textContent = laborData.name;
            document.getElementById('laborDetailCNIC').textContent = laborData.cnic;
            document.getElementById('laborDetailAge').textContent = laborData.age;
            document.getElementById('laborDetailGender').textContent = laborData.gender;
            document.getElementById('laborDetailMarital').textContent = laborData.marital;
            document.getElementById('laborDetailExperience').textContent = laborData.experience;
            document.getElementById('laborDetailSkills').textContent = laborData.skills;
            document.getElementById('laborDetailPrevWork').textContent = laborData.prevWork;
            document.getElementById('laborDetailEducation').textContent = laborData.education;
            document.getElementById('laborDetailLanguages').textContent = laborData.languages;
            document.getElementById('laborDetailHealth').textContent = laborData.health;
            document.getElementById('laborDetailEmergencyName').textContent = laborData.emergencyName;
            document.getElementById('laborDetailEmergencyRelation').textContent = laborData.emergencyRelation;

            // Show the modal
            const modal = new bootstrap.Modal(document.getElementById('laborDetailsModal'));
            modal.show();
        }
    </script>
<script src="js/cursor-effects.js"></script></body>
</html>
